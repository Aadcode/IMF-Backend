generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

enum StatusEnum {
    Available
    Deployed
    Destroyed
    Decommissioned
}

model User {
    id     Int        @id @default(autoincrement())
    name   String
    status StatusEnum
}
